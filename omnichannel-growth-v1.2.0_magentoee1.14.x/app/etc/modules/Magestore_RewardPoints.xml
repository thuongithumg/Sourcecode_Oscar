<config>
    <modules>
        <Magestore_RewardPoints>
            <active>true</active>
            <codePool>local</codePool>
            <aliasName>Reward Points</aliasName>
            <erp>
                <isERPmodule>true</isERPmodule>
                <alias_name>Reward Points</alias_name>
                <icon>rewardpoints</icon>
                <default_page>adminhtml/reward_dashboard</default_page>
            </erp>            
        </Magestore_RewardPoints>
        <!-- Disable Magestore_Customerreward -->
        <Magestore_Customerreward>
            <active>false</active>
        </Magestore_Customerreward>
        <!-- Change Magento EE Reward code pool -->
        <Enterprise_Reward>
            <codePool>local</codePool>
        </Enterprise_Reward>
    </modules>
</config>
